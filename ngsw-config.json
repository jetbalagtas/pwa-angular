{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": ["https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css"]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "posts-images",
      "urls": ["https://firebasestorage.googleapis.com/"],
      "version": 0,
      "cacheConfig": {
        "maxSize": 40,
        "maxAge": "3d12h",
        "timeout": "5s30u",
        "strategy": "performance"
      }
    }
  ]
}
